{
    "name": "example_multi_stack",
    "services": ["kibana", "redis"],
    "stack": [
        {
            "name": "elasticsearch",
            "base": "https://raw.githubusercontent.com/guilledk/dockerstack/master/library/elasticsearch.json"
        },
        {
            "name": "kibana",
            "base": "https://raw.githubusercontent.com/guilledk/dockerstack/master/library/kibana.json",

            "host": "127.0.0.1"
        },
        {
            "name": "redis",
            "base": "https://raw.githubusercontent.com/guilledk/dockerstack/master/library/kibana.json",

            "host": "127.0.0.1"
        }

    ]
}
